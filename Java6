import java.util.*;
public class Java8
{
 public static void main(String[] args)
 {
  int n,i,result=1;
  Scanner sc=new Scanner(System.in);
  System.out.println("Enter the n value:");
  n=sc.nextInt();
  for(i=1;i<=n;i++)
  {
   result=result*i;
  }
  System.out.println("Factorial for given value is:");
  System.out.println(+result);
 }
}
                             


 


