import java.util.Scanner;
class Java4
{
public static void main(String[] args){
int count=0,i;
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();

System.out.println("Enter n value:");

for(i=2;i<=n;i++){

if(n%i==0){
count++;
}
}
if(count>1){
System.out.println("Not a prime");
}
else{
System.out.println("Prime");
}
}
}
