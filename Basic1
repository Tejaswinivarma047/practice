import java.util.*;
public class Java1
{
 public static void main(String[] args)
 {
  int n;
  Scanner sc=new Scanner(System.in);
  System.out.println("Enter the n value");
  n=sc.nextInt();
  if(n%2==0)
  {
   System.out.println("The entered value is even");
  }
  else
  {
   System.out.println("The entered value is odd");
  }
 }
}
